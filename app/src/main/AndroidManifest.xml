<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projeto_mulher">
<uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/calculadora"
        android:label="@string/app_name"
        android:roundIcon="@drawable/calculadora"
        android:supportsRtl="true"
        android:theme="@style/Theme.Projetomulher">
        <activity
            android:name=".telas.CalculadoraTela"
            android:exported="true"
            android:theme="@style/CalcTheme"/>
        <activity
            android:name=".telas.Principal"
            android:exported="true"
            android:theme="@style/Theme.Projetomulher2" />
        <activity
            android:name=".telas.Cadastro5"
            android:exported="true" />
        <activity
            android:name=".telas.Cadastro4"
            android:exported="true" />
        <activity
            android:name=".telas.Cadastro3"
            android:exported="true" />
        <activity
            android:name=".telas.Cadastro2"
            android:exported="true" />
        <activity
            android:name=".telas.Cadastro1"
            android:exported="true" />
        <activity
            android:name=".telas.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>